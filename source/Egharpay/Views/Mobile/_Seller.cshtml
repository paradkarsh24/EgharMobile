@model Egharpay.Models.MobileViewModel
<div id="tab-5" class="tab-pane xs-mt-10">
    <div ng-controller="MobileController as model">
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <h5>Sellers available</h5>
                        </div>
                        <div class="ibox-content">
                            <div class="row">
                                <div class="col-sm-5">
                                    <div class="input-group">
                                        <input type="text" placeholder="Search" id="txtSearchPincode" ng-model="searchField" ng-keyup="$event.keyCode == 13 && model.searchSeller(searchField)" class="input-sm form-control"> <span class="input-group-btn">
                                            <button type="button" class="btn btn-sm btn-danger" ng-click="model.searchSeller(searchField)"> Go!</button>
                                        </span>
                                    </div>
                                </div>
                                <a ng-click="model.addPincode();" id="findZipCode">Get pincode from current location</a>
                            </div>
                            <div class="table-responsive">
                                <table class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th ng-click="model.order('Name')" ng-class="model.orderClass('Name')">Name</th>
                                            <th ng-click="model.order('RegistrationNumber')" ng-class="model.orderClass('RegistrationNumber')">Registration No.</th>
                                            <th ng-click="model.order('Owner')" ng-class="model.orderClass('Owner')">Owner</th>
                                            <th ng-click="model.order('Address1')" ng-class="model.orderClass('Address1')">Address</th>
                                            <th ng-click="model.order('Pincode')" ng-class="model.orderClass('Pincode')">Pincode</th>
                                        </tr>
                                    </thead>
                                    <tr ng-repeat="seller in model.sellers" class="odd gradeX" ng-click="$event.originalEvent.ngClickHandled">
                                        <td><input type="checkbox" class="myCheckBox" checklist-model="model.selectedSellers" checklist-value="seller.SellerId" ng-click="model.canWeAssign()"></td>
                                        <td><span ng-bind="seller.Name"></span></td>
                                        <td><span ng-bind="seller.RegistrationNumber"></span></td>
                                        <td><span ng-bind="seller.Owner"></span></td>
                                        <td><span ng-bind="seller.Address1"></span> <span ng-bind="mobile.Address2"></span> <span ng-bind="mobile.Address3"></span> <span ng-bind="mobile.Address4"></span></td>
                                        <td><span ng-bind="seller.Pincode"></span></td>
                                    </tr>
                                </table>
                                <button type="button" id="btnRequestSeller" class="btn btn-sm btn-danger" ng-click="model.requestMobile(@Model.MobileId)" ng-disabled="model.isAssignButtonEnable">Request Seller</button>
                                <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#myModal5">
                                    Large Modal
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

