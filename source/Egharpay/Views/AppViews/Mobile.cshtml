@{
    ViewBag.Title = "Mobile";
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Profile</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Dashboard_1", "Dashboards")">Home</a>
            </li>
            <li>
                <a>Extra Pages</a>
            </li>
            <li class="active">
                <strong>Profile</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">
    </div>
</div>
<div class="wrapper wrapper-content">
    <div class="row animated fadeInRight">
        <div class="col-md-4">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h1>Samsung Galaxy Note8</h1>
                </div>
                <div>
                    <div class="ibox-content no-padding border-left-right">
                        <img alt="image" class="img-responsive" src="~/Images/profile_big.jpg">
                    </div>

                    <div class="ibox-content profile-content">
                        <div class="row m-t-lg">
                            <div class="col-md-4">
                                <button class="btn btn-default dim btn-large-dim" type="button"><i class="fa fa-camera"></i></button>
                                <p>
                                    <h3>
                                        <strong>12MP/8MP</strong>
                                    </h3>2160p
                                </p>
                            </div>
                            <div class="col-md-4">
                                <button class="btn btn-default dim btn-large-dim" type="button"><i class="fa fa-microchip"></i></button>
                                <p><h3><strong>6GB RAM</strong></h3>32GB ROM</p>
                            </div>
                            <div class="col-md-4">
                                <button class="btn btn-default dim btn-large-dim" type="button"><i class="fa fa-mobile"></i></button>
                                <p><h3><strong>6.3"</strong></h3> Display</p>
                            </div>
                        </div>
                        <div class="row m-t-lg">
                            <div class="col-md-4">
                                <button class="btn btn-default dim btn-large-dim" type="button"><i class="fa fa-floppy-o"></i></button>
                                <p><h3><strong>32GB</strong></h3> Internal Memory</p>
                            </div>
                            <div class="col-md-4">
                                <button class="btn btn-default dim btn-large-dim" type="button"><i class="fa fa-wrench"></i></button>
                                <p><h3><strong>SnapDragon 835</strong></h3> Processor</p>
                            </div>
                            <div class="col-md-4">
                                <button class="btn btn-default dim btn-large-dim" type="button"><i class="fa fa-thermometer-2"></i></button>
                                <p>
                                    <h3>
                                        <strong>3300mAh</strong>
                                    </h3>Li-Ion Battery
                                </p>
                            </div>
                        </div>
                        <h4>Samsung Galaxy Note8</h4>
                        <p>
                            <i class="fa fa-briefcase"></i><strong> Exp. release 2017, September</strong>
                        </p>
                        <p>
                            <i class="fa fa-android"></i><strong> Android 7.1.1</strong>
