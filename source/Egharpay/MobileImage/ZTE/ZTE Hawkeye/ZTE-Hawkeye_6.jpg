			 filter_data.price_start = data.from;
			 filter_data.price_end = data.to;
		},
		onFinish: function (data) {
			filter_data.price_start = data.from;
			filter_data.price_end = data.to;
			
			filter.send(); 
		}
		});
		
		$("#ram").ionRangeSlider({
