	            filter_data.brand_id = $('select').multipleSelect('getSelects');
	            filter.send();
	        },
	        onUncheckAll:function(view) {
	            filter_data.brand_id = $('select').multipleSelect('getSelects');
	            filter.send();
	        }
    	});

		$("#price_ranges").ionRangeSlider({
