<script type="text/javascript">
(function($) {

var filter_data = {brand_id:[],router:router,slug:slug};

var filter = {
	init : function(){
		$('input:checkbox').on("change",function(){
			if ($(this).is(':checked')) 
			{
				filter_data.brand_id.push($(this).val());
			}
			else 
			{
				if ((index = filter_data.brand_id.indexOf($(this).val())) !== -1)
				{
					filter_data.brand_id.splice(index, 1);
				}
            }
			filter.send();
        });
 		
