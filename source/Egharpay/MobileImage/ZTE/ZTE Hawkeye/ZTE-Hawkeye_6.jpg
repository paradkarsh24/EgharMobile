			open_switch('compare');
        }
    });
}
function remove_compare(id)
{
    var dataString = 'compare_remove='+id;
    $.ajax({
        type: "POST",
        url: site_url+"ajax.php",
        data: dataString,
        cache: false,
       
        success: function(html)
        {
            $('#compare_panel').html(html);
            $('#total_compare').html($('#t_compare').val());
			open_switch('compare');
           
        }
    });
}
function close_switch(item){
    $(".showSwitcher").show();
    $(".switcher").hide();  
}

function open_switch(item){
    $("#switcher_"+item).show();
    $(".showSwitcher").hide();
}
