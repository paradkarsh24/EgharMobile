			if($(this).attr('action') == '/search'){
				e.preventDefault();
				window.location = '/search/'+$('#search-field').val();
			}else{
				// uses search choice input - here we take it out of the DOM because it was necessary for choosing url route but we don't want GET params from it
				$('#search-type-wrapper').remove();
				return true;
			}
		});
