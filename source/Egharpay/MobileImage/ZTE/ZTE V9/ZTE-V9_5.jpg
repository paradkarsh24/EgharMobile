						function bindLeftClick(){
							$(document).one('mousedown', function(event){
								if (event.which == 1){
									var target = $(event.target);
									if (!target.parents(parent).length){
										$(child).css('display','none');
										$(parent).css('background','');
									}
								}else{
									bindLeftClick();
								}
							});
						}
