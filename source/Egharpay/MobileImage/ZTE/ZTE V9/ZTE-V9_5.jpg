			$('#search-type-wrapper').show();
		});

		$('.search-type-choice').on('change', function(){
			if($(this).attr('id') == 'search-type-games'){
				document.cookie = 'selected-search-type=;path=/;expires=Thu, 01 Jan 1970 00:00:01 GMT';
			}else{
				var expireDate = new Date();
				expireDate.setTime(expireDate.getTime()+(365*24*60*60*1000)); // set to expire in 1 year
				document.cookie = 'selected-search-type='+$(this).attr('id')+';path=/;expires='+expireDate.toGMTString()+';';
			}
		});


		$('html').on('click', function(event){
			var target = $(event.target);
			if(!target.is('#search-type-wrapper, #search-field')
				&& target.parents('#search-type-wrapper').length === 0
			&& !$('#search-field').is(':focus')){ // this condition necessary due to (jquery?) bug where click is fired when pressing enter in search input
				$('#search-type-wrapper').hide();
			}
		});
		$('.search-type-choice').on('change', function(){
			$('#search-game-debate').attr('action', $(this).val());
		});

		$('.search-type-choice[data-preselect="true"]').trigger('click');
	});
</script>
