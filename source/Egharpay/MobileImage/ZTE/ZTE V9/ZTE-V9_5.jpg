	var setAllDiscussionQtips = function(){
		discussionRightSideQTips();
		discussionFPSQtips();
		discussionDownBtnQtips();
		discussionShareIconQtips();
	};
	var setClearDiscussionNotificationsClick = function(){
		$('.clear-discussions').click(function(){
			$('.discussion-load').html('<div class="css-spinner-6 fll"></div>');
			$.ajax({
				url: '/clear/discussion/notifications',
				type: 'POST',
				data : {"id":"82","d_type":"tablet"},
				dataType: 'JSON',
				success: function(data){
					$('.discussAllCommentArea p').css('color','#555');
					$('.discussion-load').html('');
				}
			});
		});
	};
	var discussionSetReplyClickToggleHasRun = false;
	var discussionSetReplyClickToggle = function(){
		if(discussionSetReplyClickToggleHasRun)
			return false;
		$('body').on('click', '.discuss-reply-btn', function(){
			var state = $(this).data('state');
			switch(state){
				case 1:
				case undefined: 
					$('.discuss-reply-wrapper[data-discussid='+$(this).attr('data-discussid')+']').show('fast');
					$(this).data('state', 2);
					break;
				case 2:
					$('.discuss-reply-wrapper[data-discussid='+$(this).attr('data-discussid')+']').hide('fast');
					 $(this).data('state', 1);
					break;
			}
		});
		discussionSetReplyClickToggleHasRun = true;
	};
	var discussionSetTextareaCount = function(){
		var textareaCountOpts = {
			'maxCharacterSize': 0,
			'originalStyle': 'originalDisplayInfo',
			'warningStyle': 'warningDisplayInfo',
			'warningNumber': 40,
			'displayFormat': '#input Characters | #left Characters Left | #words Words'
		};
		$('.discussAllTextArea').each(function(){$(this).textareaCount(textareaCountOpts);});
	};
	var discussionThumbUpdateToast = function(json){
		if (typeof json.success !== 'undefined'){
			if (json.success.locked==1){
				toastr.warning('Target member\'s reputation is locked, your thumb will be applied when the lock has been released.');
			}
			if (json.success.upVotedBefore==1)
				toastr.success('Up thumbed again, your thumb has been refreshed');
			if (json.success.downVotedBefore==1)
				toastr.warning('Down thumbed again, your thumb has been refreshed');
			if(json.success.downVotedBefore==1||json.success.upVotedBefore==1)
				return;
			if(json.success.direction===1)
				toastr.success('Up thumbed');
			if (json.success.direction===0)
				toastr.warning('Down thumbed');
		}else if(typeof json.error !== 'undefined'){
			toastr.warning($('<div />').text(json.error).html());
		}
		if (typeof json.warnings !== 'undefined'){
			json.warnings.forEach(function(warning){
				toastr.warning(warning.text, null, {"timeOut": "20000"});
			});
		}
	};
	var discussionCanRunQtips = function(){
		$('.canRunVoteBtn').qtip({
			style: {
				classes: 'qtip-dark qtip-shadow larger-qtip-text',
				tip: {
					width: 12,
					height: 12
				}
			},
			position: {
				my: 'bottom center',
				at: 'top center'
			},
			show:{
				delay: 500
			}
		});
	};
	var discussionSetupAskCanRun = function(){
		if(discussionCalledAll)
			return false;
		var getCanRunText = function(){
			return "Can I run "+ $(document).find('.ask-can-run-btn').attr('data-game') +"?";
		};

		$('body').on('click', '.discuss-fps-submit, .discuss-fps-act-as-btn', function(e){
			setTimeout(function(){
				openChrome(function(){
					highlightTimer = setTimeout(function(){
						$('.chrome-add').effect('highlight', {color: '#FFF'}, 1500);
					}, 500);
					$('#tab-fps').trigger('click');
				});
			}, 1);
		});

		$('body').on('click', '.ask-can-run-btn', function(e){
			e.preventDefault();
			var canRunCommentText = getCanRunText();
			var cpuName = $(this).attr('data-cpu');
			var gpuName = $(this).attr('data-gpu');
			var ramAmount = $(this).attr('data-ram');
			if(ramAmount == "" || cpuName == "" || gpuName == ""){
				toastr.error("Please complete your member rig so that other members can see your hardware specifications.");
				return false;
			}
			$(this).hide();
			$('.close-ask-can-run').show();
			$('.ask-can-run-rig').show();
			$('.ask-can-run-rig-cpu').text(cpuName);
			$('.ask-can-run-rig-gpu').text(gpuName);
			$('.ask-can-run-rig-ram').text(ramAmount);
			$('.can-i-run-question').val("1");
			if(!$('.discussAllTextArea').val().length > 0){
				$('.discussAllTextArea').val(canRunCommentText);
			}
		});
		$('body').on('change', '.canRunUserChoice', function(){
			$.ajax({
				url: '/discuss/vote-can-run/text',
				dataType: 'json',
				type: 'GET',
				data: {
					g_id: $('#container1').attr('data-gid'),
					canRunResolution: $(document).find('.canRunResolution').val(),
					canRunMinFPS: $(document).find('.canRunMinFPS').val(),
					canRunGraphicSetting: $(document).find('.canRunGraphicSetting').val()
				}
			}).done(function(data){
				if(typeof data !== undefined && typeof data.text == 'string'){
					$('.ask-can-run-text').html(data.text);
				}else{
					toastr.error('Something went wrong with the request');
				}
			}).error(function(){
				toastr.error('Something went wrong with the request');
			});
		});
		$('body').on('click', '.close-ask-can-run', function(e){
			$('.ask-can-run-btn').show();
			$('.close-ask-can-run').hide();
			var canRunCommentText = getCanRunText();
			e.preventDefault();
			if($('.discussAllTextArea').val() == canRunCommentText){
				$('.discussAllTextArea').val("");
			}
			$('.can-i-run-question').val("0");
			$('.ask-can-run-rig').hide();
		});
		$('body').on('click', '.canRunVoteBtn', function(){
			var d_id = $(this).attr('data-d_id');
			var $discussion = $(this).closest('.discussAllOuter');
			$.ajax({
				url: '/discuss/vote-can-run',
				type: 'POST',
				dataType: 'json',
				data: {
					d_id: $(this).attr('data-d_id'),
					voteVal: $(this).attr('data-val')
				}
			}).done(function(data){
				if(typeof data.success == 'undefined'){
					toastr.error("There was an error contacting Game-Debate")
				}else if(data.success == 1){
					toastr.success(data.message);
					$('.canRunContainer[data-d_id = '+d_id+'] .canRunVoting').remove();
					$('.canRunContainer[data-d_id = '+d_id+'] .canRunRig').after(data.buttonHTML);
					$('.canRunContainer[data-d_id = '+d_id+'] .canRunResult').remove();
					$('.canRunContainer[data-d_id = '+d_id+']').append(data.resultHTML);
					discussionCanRunQtips();
					// var $discussion = that.closest('.discussAllOuter');
					var $textarea = $discussion.find('.discussAllTextArea');
					if(data.voteAdded == 1 && !$textarea.is(':visible')){
						$discussion.find('.discuss-reply-btn').trigger('click');
						$textarea.text("I voted "+(data.voteVal == 1? 'yes' : 'no')+" because...");
					}
				}else{
					toastr.error(data.message);
				}
			}).error(function(){
				toastr.error("There was an error contacting Game-Debate")
			});
		});
	};
		var discussionVoting = function(){
		$('.discussion-voting').click(function(){
			if ($(this).find('div').hasClass('disabledVotes')) return false;
			var d_id = $(this).attr('data-did');
			var loadingSpace = $('#vote-score-'+d_id);
			var height = loadingSpace.height();
			var d_vote = $(this).attr('data-vote');
			loadingSpace.html('<img class="votes-loading" style="height:16px;width:16px" src="/images/ajax-loader (6).gif">');
			var upArrow = $('#discussion-id-'+d_id).find('.arrow-34-icon-up');
			var downArrow = $('#discussion-id-'+d_id).find('.arrow-34-icon-down');

			var promotedUpArrow = $('#promoted-up-'+d_id);
			var promotedCount = $('#promoted-count-'+d_id);
			$.ajax({
				url: '/routes/votes/castVote',
				data: {'vote':d_vote,'d_id':d_id},
				dataType: 'json',
				type: 'POST',
				success: function(data){
					$('.votes-loading').remove();
					if(typeof data.changeArrowColor !== 'undefined' && data.changeArrowColor){
						if (data.mt_vote==1){
